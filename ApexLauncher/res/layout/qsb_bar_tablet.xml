<?xml version="1.0" encoding="utf-8"?>
<com.android.launcher2.SearchDropTargetBar android:focusable="false" style="@style/TabletQSBBar"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <include android:id="@id/qsb_search_bar" layout="@layout/search_bar_tablet" />
    <LinearLayout android:id="@id/drag_target_bar" style="@style/TabletSearchDropTargetBar">
        <FrameLayout android:layout_weight="1.0" style="@style/TabletDropTargetButtonContainer">
            <com.android.launcher2.DeleteDropTarget android:id="@id/delete_target_text" android:text="@string/delete_zone_label_workspace" android:drawableLeft="@drawable/remove_target_selector" style="@style/TabletDropTargetButton" />
        </FrameLayout>
        <FrameLayout android:layout_weight="1.0" style="@style/TabletDropTargetButtonContainer">
            <com.android.launcher2.InfoDropTarget android:id="@id/info_target_text" android:text="@string/info_target_label" android:drawableLeft="@drawable/info_target_selector" style="@style/TabletDropTargetButton" />
        </FrameLayout>
    </LinearLayout>
</com.android.launcher2.SearchDropTargetBar>
